{
  "name": "crm-web",
  "version": "1.16.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prebuild": "npm run codegen",
    "lint": "next lint --format stylish",
    "test:e2e": "playwright test",
    "test-headed:e2e": "playwright test --headed",
    "show-e2e-test-report": "playwright show-report",
    "test:e2e-with-traces": "playwright test --trace on",
    "eslint": "eslint .",
    "typecheck": "tsc --noEmit",
    "codegen": "npm run codegen:crm-service && npm run codegen:documents && npm run codegen:search-criteria && npm run codegen:metafactory-core && npm run codegen:image-service && npm run codegen:workflow-task-management",
    "codegen:metafactory-core": "openapi-typescript 'https://app.swaggerhub.com/apiproxy/registry/MetaFactoryBV/CarStock/1.0.51?resolved=true&flatten=true&pretty=true' -o ./generated/metafactory-service-types.ts",
    "codegen:search-criteria": "openapi-typescript 'https://am-i-b-v.github.io/car-stock-search-criteria-service/openapi.yaml?resolved=true&flatten=true&pretty=true' -o ./generated/search-criteria-types.ts",
    "codegen:crm-service": "openapi-typescript 'https://am-i-b-v.github.io/crm-service/openapi.yaml?resolved=true&flatten=true&pretty=true' -o ./generated/crm-service-types.ts",
    "codegen:documents": "openapi-typescript 'https://am-i-b-v.github.io/car-stock-documents-service/openapi.yaml?resolved=true&flatten=true&pretty=true' -o ./generated/documents-types.ts",
    "codegen:image-service": "openapi-typescript 'https://am-i-b-v.github.io/car-stock-image-service/openapi.yaml?resolved=true&flatten=true&pretty=true' -o ./generated/image-service-types.ts",
    "codegen:workflow-task-management": "openapi-typescript 'https://app.swaggerhub.com/apiproxy/registry/MetaFactoryBV/WorkflowTaskManagement/1.0.0?resolved=true&flatten=true&pretty=true' -o ./generated/workflow-task-management-types.ts",
    "postinstall": "npm run codegen",
    "prepare": "husky install",
    "prettier": "prettier --write \"src/**/*\""
  },

  "dependencies": {

    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "@mui/material-nextjs": "^5.15.11",
    "@mui/x-date-pickers": "^6.18.7",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.0",
    "@opentelemetry/id-generator-aws-xray": "^1.2.2",
    "@opentelemetry/instrumentation-aws-sdk": "^0.42.0",
    "@opentelemetry/instrumentation-undici": "^0.3.0",
    "@opentelemetry/propagator-aws-xray": "^1.25.0",
    "@opentelemetry/resources": "^1.25.0",
    "@opentelemetry/sdk-node": "^0.52.0",
    "@opentelemetry/sdk-trace-node": "^1.23.0",
    "@opentelemetry/semantic-conventions": "^1.23.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@tanstack/react-query": "^5.45.1",
    "class-variance-authority": "^0.7.0",
    "country-data": "^0.0.31",
    "country-list": "^2.3.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "dompurify": "^3.1.4",
    "framer-motion": "^10.18.0",
    "jose": "^5.2.2",
    "jwt-decode": "^4.0.0",
    "lodash.debounce": "^4.0.8",
    "next": "^14.2.3",
    "next-intl": "^3.4.0",
    "openapi-typescript": "5.4.0",
    "quill-mention": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.10",
    "react-hook-form": "^7.49.3",
    "react-icons": "^4.12.0",
    "react-phone-number-input": "^3.3.9",
    "react-quill": "^2.0.0",
    "react-toastify": "^9.1.3",
    "server-only": "^0.0.1",
    "sharp": "^0.33.3",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@next/bundle-analyzer": "^14.0.4",
    "@playwright/test": "^1.42.1",
    "@types/country-data": "^0.0.5",
    "@types/country-list": "^2.1.4",
    "@types/dompurify": "^3.0.5",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.3",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "release-it": "^16.1.0",
    "tailwindcss": "^3.4.3",
    "tsc-files": "^1.1.4",
    "typescript": "^5.1.3"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "release-it": {
    "git": {
      "commitMessage": "[actions skip]chore: release ${version}"
    },
    "github": {
      "release": false
    },
    "npm": {
      "publish": false
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss}": [
      "npm run eslint"
    ],
    "src/**/*.{ts,tsx}": [
      "tsc-files --noEmit"
    ]
  }
}
